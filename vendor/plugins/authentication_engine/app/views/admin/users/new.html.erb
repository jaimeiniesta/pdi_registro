<%# render :partial => 'admin/shared/restful_links', :object => @user %>
<% title "Registrar una cuenta" %>

<%= error_messages_for :user %>

<% form_for [:admin, @user] do |f| %>
  <p>
    <%= f.label :name, "Nombre" %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email, "Email" %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit "Nueva Cuenta" %>
  </p>
<% end %>
